{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learnJenkinsapp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnJenkinsApp",
            "image": "703420526575.dkr.ecr.us-west-2.amazonaws.com/myjenkinsapp:1.0.126",
            "portMappings": [
                {
                    "name": "nginx-80-tip",
                    "containerPort": 80,
                    "protocol": "tcp",
                    "hostPort": 80,
                    "appProtocol": "http"
                    
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::703420526575:role/ecsTaskExecutionRole"
}